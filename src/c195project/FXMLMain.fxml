   <children>
      <Label layoutX="14.0" layoutY="6.0" text="Main" />
      <AnchorPane layoutX="28.0" layoutY="23.0" prefHeight="367.0" prefWidth="200.0">
         <children>
            <DatePicker fx:id="datePicker" layoutX="-4.0" layoutY="14.0" onAction="#selectedDateHandler" prefHeight="25.0" prefWidth="200.0" />
