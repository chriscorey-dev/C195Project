            <ListView fx:id="appointmentsListView" layoutY="56.0" prefHeight="200.0" prefWidth="200.0" />
            <Button fx:id="addAppointmentButton" layoutX="6.0" layoutY="256.0" mnemonicParsing="false" onAction="#addAppointmentHandler" text="New Appointment" />
            <Button layoutX="6.0" layoutY="281.0" mnemonicParsing="false" text="Edit Appointment" />
         </children>
      </AnchorPane>
      <AnchorPane layoutX="306.0" layoutY="23.0" prefHeight="367.0" prefWidth="200.0">
         <children>
            <ListView fx:id="customersListView" layoutY="12.0" prefHeight="244.0" prefWidth="200.0" />
            <Button fx:id="addCustomerButton" layoutX="14.0" layoutY="256.0" mnemonicParsing="false" onAction="#addCustomerHandler" text="New Customer" />
