<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.DatePicker?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.Spinner?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane id="AnchorPane" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/10.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="c195project.FXMLAppointmentController">
   <children>
      <Label layoutX="14.0" layoutY="14.0" text="Appointment" />
      <AnchorPane layoutX="315.0" layoutY="76.0" prefHeight="85.0" prefWidth="135.0">
         <children>
            <Label text="Date" />
            <DatePicker id="d" fx:id="date" layoutY="17.0" prefHeight="25.0" prefWidth="139.0" />
            <Label layoutY="42.0" text="Start Time" />
            <TextField fx:id="startTime" layoutY="59.0" prefHeight="25.0" prefWidth="67.0" />
            <Label layoutX="75.0" layoutY="42.0" text="End Time" />
            <TextField fx:id="endTime" layoutX="73.0" layoutY="59.0" prefHeight="25.0" prefWidth="67.0" />
         </children>
      </AnchorPane>
      <AnchorPane layoutX="121.0" layoutY="65.0" prefHeight="32.0" prefWidth="54.0">
         <children>
            <Label layoutX="3.0" layoutY="6.0" text="Title" />
            <TextField fx:id="title" layoutX="2.0" layoutY="23.0" />
            <Label layoutX="3.0" layoutY="48.0" text="Description" />
            <TextArea fx:id="description" layoutX="2.0" layoutY="65.0" prefHeight="58.0" prefWidth="149.0" />
         </children>
      </AnchorPane>
      <Spinner fx:id="customer" layoutX="313.0" layoutY="185.0" />
      <Label layoutX="309.0" layoutY="167.0" text="Customer" />
      <AnchorPane layoutX="127.0" layoutY="200.0" prefHeight="200.0" prefWidth="140.0">
         <children>
            <Label layoutX="-3.0" layoutY="84.0" text="Type" />
            <TextField fx:id="type" layoutX="-3.0" layoutY="101.0" prefHeight="25.0" prefWidth="130.0" />
            <Label layoutX="-1.0" layoutY="121.0" text="URL" />
            <TextField fx:id="url" layoutX="-3.0" layoutY="138.0" prefHeight="25.0" prefWidth="130.0" />
            <Label layoutX="-3.0" text="Location" />
            <TextField fx:id="location" layoutX="-3.0" layoutY="17.0" prefHeight="25.0" prefWidth="130.0" />
            <Label layoutX="-1.0" layoutY="42.0" text="Contact" />
            <TextField fx:id="contact" layoutX="-3.0" layoutY="59.0" prefHeight="25.0" prefWidth="130.0" />
         </children>
      </AnchorPane>
      <Button fx:id="submitAppointmentButton" layoutX="315.0" layoutY="275.0" mnemonicParsing="false" onAction="#submitAppointmentHandle" text="Save" />
      <Button fx:id="cancelAppointmentButton" layoutX="374.0" layoutY="275.0" mnemonicParsing="false" onAction="#cancelAppointmentHandle" text="Cancel" />
   </children>
</AnchorPane>
